<?xml version="1.0"?>
<configuration>
  <configSections>
    <section name="urlrewritingnet"
    restartOnExternalChanges="true"
    requirePermission="false"
    type="UrlRewritingNet.Configuration.UrlRewriteSection,UrlRewritingNet.UrlRewriter"/>
  </configSections>

  <connectionStrings>
    <add name="forums" connectionString="SERVER=CIDEAN\SQL2005;Database=forums;UID=xxxxx;PWD=xxxxx" providerName="System.Data.SqlClient"/>
  </connectionStrings>

  <urlrewritingnet rewriteOnlyVirtualUrls="true"

   xmlns="http://www.urlrewriting.net/schemas/config/2006/07" >
    <rewrites>


      <add name="home"
      virtualUrl="~/default.aspx"
      rewriteUrlParameter="ExcludeFromClientQueryString"
      destinationUrl="~/viewtopics.aspx?forum=requests"
      ignoreCase="true" />

      <add name="addtopic"
        virtualUrl="~/addtopic/([^/]+)/?"
        rewriteUrlParameter="ExcludeFromClientQueryString"
        destinationUrl="~/addtopic.aspx?forum=$1"
        ignoreCase="true" />

      <add name="post"
      virtualUrl="^~/([^/]+)/([^(/|\-)]+)-([^/]+)"
      rewriteUrlParameter="ExcludeFromClientQueryString"
      destinationUrl="~/viewposts.aspx?topic=$2"
      ignoreCase="true" />

      <add name="topic"
      virtualUrl="~/([^/]+)/?"
      rewriteUrlParameter="ExcludeFromClientQueryString"
      destinationUrl="~/viewtopics.aspx?forum=$1"
      ignoreCase="true" />

      



    </rewrites>
  </urlrewritingnet>
   

  
  <system.web>
    <httpRuntime requestValidationMode="2.0" />
    <compilation debug="true" targetFramework="4.0" />
   
    <authentication mode="Forms" >

      <forms loginUrl="/login"
        protection="All"
        timeout="30"
        name=".ASPXAUTH"
        path="/"
        domain="cidean.com"
        requireSSL="false"
        slidingExpiration="true"
        defaultUrl="/main"
        cookieless="UseDeviceProfile"
        enableCrossAppRedirects="true" />
    </authentication>
    <machineKey
      validationKey="C50B3C89CB21F4F1422FF158A5B42D0E8DB8CB5CDA1742572A487D9401E3400267682B202B746511891C1BAF47F8D25C07F6C39A104696DB51F17C529AD3CABE"
      decryptionKey="8A9BE8FD67AF6979E7D20198CFEA50DD3D3799C77AF2B72F"
      validation="SHA1" />
 
    <pages validateRequest="false" />
  </system.web>

  <system.webServer>
    <modules  runAllManagedModulesForAllRequests="true">
      <add name="UrlRewriteModule" type="UrlRewritingNet.Web.UrlRewriteModule, UrlRewritingNet.UrlRewriter" />
    </modules>
  </system.webServer>
</configuration>
